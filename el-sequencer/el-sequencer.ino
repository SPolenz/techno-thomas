#define CHANNEL_A 2
#define CHANNEL_B 3
#define CHANNEL_C 4
#define CHANNEL_D 5
#define CHANNEL_E 6
#define CHANNEL_F 7
#define CHANNEL_G 8
#define CHANNEL_H 9

void setup() {
  PCICR |= (1 << PCIE1); // enable pin change interrupts
  PCMSK1 |= (1 << PCINT10); // enable PCI for pin A2

  pinMode(CHANNEL_A, OUTPUT);
  pinMode(CHANNEL_B, OUTPUT);
  pinMode(CHANNEL_C, OUTPUT);
  pinMode(CHANNEL_D, OUTPUT);
  pinMode(CHANNEL_E, OUTPUT);
  pinMode(CHANNEL_F, OUTPUT);
  pinMode(CHANNEL_G, OUTPUT);
  pinMode(CHANNEL_H, OUTPUT);

  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay();
  display.drawBitmap(0, 0, modeImages[0], SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
  display.display();
  Serial.begin(9600);

  for (int i = 4; i >= 0; i--) {
    lightWiresInsideOut(i);
    delay(250);
  }
}

byte mode = 0;
String modeNames[] = { "Peak to P.", "RMS", "Avg. Peak", "manual 1", "manual 2" };
int maxInputsByMode[] = { 420, 80, 80, 450, 0 };
int minInputsByMode[] = { 160, 20, 20, 0, 0 };
int value = 0;
int xValue = 0;
int yValue = 0;
boolean buttonPressed = false;

ISR(PCINT1_vect) {
  static unsigned long lastTime = 0;
  unsigned long currentTime = millis();
  if (currentTime - lastTime > 100) {
    if (digitalRead(A2) == HIGH) {
      buttonPressed = true;
    }
  }
  lastTime = currentTime;
}

#define JOYSTICK_Y A3
#define JOYSTICK_X A6
#define IDLE_Y 517
#define IDLE_X 493

void printMode() {
  display.clearDisplay();
  display.drawBitmap(0, 0, modeImages[mode], SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
  display.display();
}

void loop() {
  unsigned long timer = millis();
  if (buttonPressed) {
    buttonPressed = false;
    mode = (mode + 1) % 5;
    printMode();
  }
  xValue = analogRead(JOYSTICK_X);
  yValue = analogRead(JOYSTICK_Y);

  if (mode < 3 && (xValue > 800 || xValue < 200 || yValue > 800 || yValue < 200 )) {
    mode = 3;
    printMode();
  }
  
  if (mode == 0) {
    value = getPeakToPeak();
  } else if (mode == 1) {
    value = getRootMeanSquares();
  } else if (mode == 2) {
    value = getAveragePeak();
  } else if (mode == 3) {
    value = yValue - IDLE_Y;
  }

  int displayValue = map(value, minInputsByMode[mode], maxInputsByMode[mode], 0, 4);
  if (mode == 4) {
    byte pos = getJoystickPosition();
    lightWiresByPosition(pos);
  } else {
    lightWiresInsideOut(displayValue);
  }

  Serial.print(displayValue);
  Serial.print(",");
  Serial.print(maxInputsByMode[mode]);
  Serial.print(",");
  Serial.print(minInputsByMode[mode]);
  Serial.print(",");
  Serial.print(value);
  Serial.print(",");
  Serial.print(xValue, DEC);
  Serial.print(",");
  Serial.print(yValue, DEC);
  Serial.print(",");
  Serial.println(millis() - timer);
}

// counter-clockwise from POV, starting with left arm
byte getJoystickPosition() {
  byte pos = 0;
  if (yValue < 200) {
    if (xValue < 200) {
      bitSet(pos, 4);
    } else if (xValue > 800) {
      bitSet(pos, 2);
    } else {
      bitSet(pos, 3);
    }
  } else if (yValue > 800) {
    if (xValue < 200) {
      bitSet(pos, 6);
    } else if (xValue > 800) {
      bitSet(pos, 0);
    } else {
      bitSet(pos, 7);
    }
  } else {
    if (xValue < 200) {
      bitSet(pos, 5);
    } else if (xValue > 800) {
      bitSet(pos, 1);
    }
  }
  return pos;
}

int positionalGroups[8][2] = {
  { CHANNEL_H, CHANNEL_G }, // left arm
  { CHANNEL_H, CHANNEL_F }, // left arm + leg outside
  { CHANNEL_F, CHANNEL_E }, // left leg
  { CHANNEL_F, CHANNEL_C }, // both legs outside
  { CHANNEL_D, CHANNEL_C }, // right leg
  { CHANNEL_C, CHANNEL_A }, // right arm + leg outside
  { CHANNEL_B, CHANNEL_A }, // right arm
  { CHANNEL_H, CHANNEL_A }  // both arms outside
};
void lightWiresByPosition(byte pos) {
  for (int c = CHANNEL_A; c <= CHANNEL_H; c++) {
    digitalWrite(c, LOW);
  }
  for (int i = 0; i < 8; i++) {
    if (bitRead(pos, i) == 1) {
      for (int j = 0; j < 2; j++) {
        digitalWrite(positionalGroups[i][j], HIGH);
      }
    }
  }
  delay(15); // rest in final state to reduce flicker, as there is no sampling happening in this mode
}

int insideOutGroups[4][2] = {
  { CHANNEL_D, CHANNEL_E },
  { CHANNEL_C, CHANNEL_F },
  { CHANNEL_B, CHANNEL_G },
  { CHANNEL_A, CHANNEL_H }
};
void lightWiresInsideOut(int groupCount) {
  for (int i = 0; i < 4; i++) {
    int value = i < groupCount ? HIGH : LOW;
    for (int j = 0; j < 2; j++) {
      digitalWrite(insideOutGroups[i][j], value);
    }
  }
}

#define SAMPLE_WINDOW 20 // ms
#define MIKE_OUT A7

int getPeakToPeak() {
  unsigned long startMillis = millis();
  unsigned int signalMax = 0;
  unsigned int signalMin = 1023;
  while (millis() - startMillis < SAMPLE_WINDOW) {
    int sample = analogRead(MIKE_OUT);
    if (sample > signalMax) {
      signalMax = sample;
    } else if (sample < signalMin) {
      signalMin = sample;
    }
  }
  return signalMax - signalMin;
}

#define BASELINE 382

int getRootMeanSquares() {
  unsigned long startMillis = millis();
  unsigned long sumOfSquares = 0;
  unsigned int numSamples = 0;
  while (millis() - startMillis < SAMPLE_WINDOW) {
    int sample = analogRead(MIKE_OUT) - BASELINE;
    numSamples++;
    sumOfSquares += sq(sample);
  }
  int value = round(sqrt(sumOfSquares / numSamples));
  return value;
}

int getAveragePeak() {
  unsigned long startMillis = millis();
  unsigned int numSamples = 0;
  int sum = 0;
  while (millis() - startMillis < SAMPLE_WINDOW) {
    int sample = abs(analogRead(MIKE_OUT) - BASELINE);
    numSamples++;
    sum += sample;
  }
  int value = round(sum / numSamples);
  return value;
}
