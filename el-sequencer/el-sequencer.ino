#define CHANNEL_A 2
#define CHANNEL_B 3
#define CHANNEL_C 4
#define CHANNEL_D 5
#define CHANNEL_E 6
#define CHANNEL_F 7
#define CHANNEL_G 8
#define CHANNEL_H 9

#define ACTIVE_CHANNELS 6

#define MIKE_OUT A7
#define MIKE_SAMPLE_WINDOW 15 // ms
#define MIKE_BASELINE 382

#define JOYSTICK_Y A3
#define JOYSTICK_X A6
#define IDLE_Y 517
#define IDLE_X 493
#define JOYSTICK_HIGH_VALUE 1000
#define JOYSTICK_LOW_VALUE 20

#define DEBUG 1

const byte statusChannel = CHANNEL_A; // keep one channel on at all times
const byte channelOrder[ACTIVE_CHANNELS] = { CHANNEL_B, CHANNEL_C, CHANNEL_D, CHANNEL_E, CHANNEL_F, CHANNEL_G };

byte mode = 0;
int xValue = 0;
int yValue = 0;
boolean buttonPressed = false;
unsigned int beatInterval = 468; // ms

void setup() {
  PCICR |= (1 << PCIE1); // enable pin change interrupts
  PCMSK1 |= (1 << PCINT10); // enable PCI for pin A2

  pinMode(CHANNEL_A, OUTPUT);
  pinMode(CHANNEL_B, OUTPUT);
  pinMode(CHANNEL_C, OUTPUT);
  pinMode(CHANNEL_D, OUTPUT);
  pinMode(CHANNEL_E, OUTPUT);
  pinMode(CHANNEL_F, OUTPUT);
  pinMode(CHANNEL_G, OUTPUT);
  pinMode(CHANNEL_H, OUTPUT);

  digitalWrite(statusChannel, HIGH);

  initDisplay();
  playWireStartSequence();

  #ifdef DEBUG
  Serial.begin(19200);
  #endif
}

void loop() {
  unsigned long timer = millis();

  handleButton();

  xValue = analogRead(JOYSTICK_X);
  yValue = analogRead(JOYSTICK_Y);

  if (mode == 0) {
    runAveragePeakMode();
  } else if (mode == 1) {
    runPeakToPeakMode();
  } else if (mode == 2) {
    runRmsMode();
  } else if (mode == 3) {
    measureBeatInterval();
  } else if (mode == 4) {
    runBeatIntervalCycle1();
  } else if (mode == 5) {
    runBeatIntervalCycle2();
  }

  #ifdef DEBUG
  Serial.print(xValue, DEC);
  Serial.print(",");
  Serial.print(yValue, DEC);
  Serial.print(",");
  Serial.print(beatInterval);
  Serial.print(",");
  Serial.println(millis() - timer);
  #endif
}

unsigned long lastBeatTime = 0;
unsigned long totalTime = 0;
unsigned int beatCounter = 0;

void switchToMeasureBeatInterval() {
  lastBeatTime = 0;
  totalTime = 0;
  beatCounter = 0;
}

#define MIN_INTERVAL_BETWEEN_BEATS 200

void measureBeatInterval() {
  lightWiresUpToIndex(ACTIVE_CHANNELS);
  unsigned long currentTime = millis();
  unsigned long timeSinceLastBeat = currentTime - lastBeatTime;
  if (isJoystickUp() && (timeSinceLastBeat > MIN_INTERVAL_BETWEEN_BEATS)) {
    if (beatCounter > 0) {
      totalTime += timeSinceLastBeat;
      beatInterval = totalTime / beatCounter;
    }
    beatCounter++;
    lastBeatTime = currentTime;
    lightWiresUpToIndex(0);
    delay(MIN_INTERVAL_BETWEEN_BEATS / 2);
  } else {
    lightWiresUpToIndex(ACTIVE_CHANNELS);
  }
}

boolean cycleStarted = false;

void switchToBeatIntervalCycle() {
  cycleStarted = false;
}

#define TIMING_ACCURACY 5 // ms

void runWithBeatInterval(void (*lightWiresFunction)(int)) {
  unsigned long currentTime = millis();
  if (!cycleStarted && (currentTime - lastBeatTime) % beatInterval < TIMING_ACCURACY) { // wait until we catch a beat
    cycleStarted = true;
  }
  if (cycleStarted) {
    int numDelays = ACTIVE_CHANNELS * 2 - 2;
    for (int i = ACTIVE_CHANNELS; i > 1; i--) {
       lightWiresFunction(i);
       double delayTime = beatInterval / numDelays;
       int delayTimeRounded = round(delayTime);
       delay(delayTimeRounded);
    }
    for (int i = 1; i < ACTIVE_CHANNELS; i++) {
       lightWiresFunction(i);
       double delayTime = beatInterval / numDelays;
       int delayTimeRounded = round(delayTime);
       delay(delayTimeRounded);
    }
  }
}

void lightThreeWiresUpToIndex(int index) {
  lightNumWiresUpToIndex(3, index);
}

void runBeatIntervalCycle1() {
  runWithBeatInterval(&lightThreeWiresUpToIndex);
}

void runBeatIntervalCycle2() {
  runWithBeatInterval(&lightRandomWires);
}

void runAveragePeakMode() {
  int value = getAveragePeak();
  #define MIN_AVG_PEAK 45
  #define MAX_AVG_PEAK 110
  int displayValue = map(value, MIN_AVG_PEAK, MAX_AVG_PEAK, 0, ACTIVE_CHANNELS);
  displayValue = constrain(displayValue, 0, ACTIVE_CHANNELS);
  lightWiresUpToIndex(displayValue);
  #ifdef DEBUG
  Serial.print(value);
  Serial.print(",");
  Serial.print(MIN_AVG_PEAK);
  Serial.print(",");
  Serial.print(MAX_AVG_PEAK);
  Serial.print(",");
  Serial.print(displayValue);
  Serial.print(",");
  #endif
}

void runPeakToPeakMode() {
  int value = getPeakToPeak();
  #define MIN_PEAK_TO_PEAK 210
  #define MAX_PEAK_TO_PEAK 425
  int displayValue = map(value, MIN_PEAK_TO_PEAK, MAX_PEAK_TO_PEAK, 0, ACTIVE_CHANNELS);
  displayValue = constrain(displayValue, 0, ACTIVE_CHANNELS);
  lightWiresUpToIndex(displayValue);
  #ifdef DEBUG
  Serial.print(value);
  Serial.print(",");
  Serial.print(MIN_PEAK_TO_PEAK);
  Serial.print(",");
  Serial.print(MAX_PEAK_TO_PEAK);
  Serial.print(",");
  Serial.print(displayValue);
  Serial.print(",");
  #endif
}

void runRmsMode() {
  int value = getRootMeanSquares();
  #define MIN_RMS 50
  #define MAX_RMS 90
  int displayValue = map(value, MIN_RMS, MAX_RMS, 0, ACTIVE_CHANNELS);
  displayValue = constrain(displayValue, 0, ACTIVE_CHANNELS);
  lightWiresUpToIndex(displayValue);
  #ifdef DEBUG
  Serial.print(value);
  Serial.print(",");
  Serial.print(MIN_RMS);
  Serial.print(",");
  Serial.print(MAX_RMS);
  Serial.print(",");
  Serial.print(displayValue);
  Serial.print(",");
  #endif
}

void handleButton() {
  if (buttonPressed) {
    buttonPressed = false;
    switchMode();
  }
}

void switchMode() {
  mode = (mode + 1) % 6;
  printMode();
  if (mode == 3) {
    switchToMeasureBeatInterval();
  } else if (mode == 4 || mode == 5) {
    switchToBeatIntervalCycle();
  }
}

boolean isJoystickUp() {
  return yValue > JOYSTICK_HIGH_VALUE;
}

boolean isJoystickDown() {
  return yValue < JOYSTICK_LOW_VALUE;
}

boolean isJoystickRight() {
  return xValue > JOYSTICK_HIGH_VALUE;
}

boolean isJoystickLeft() {
  return xValue < JOYSTICK_LOW_VALUE;
}

void lightWiresUpToIndex(int index) {
  for (int i = 0; i < ACTIVE_CHANNELS; i++) {
    int value = i < index ? HIGH : LOW;
    digitalWrite(channelOrder[i], value);
  }
}

void lightWiresAtIndex(int index) {
  for (int i = 0; i < ACTIVE_CHANNELS; i++) {
    int value = i == index - 1 ? HIGH : LOW;
    digitalWrite(channelOrder[i], value);
  }
}

void lightNumWiresUpToIndex(int num, int index) {
  for (int i = 0; i < ACTIVE_CHANNELS; i++) {
    int value = ((index > i) && (i + num >= index)) ? HIGH : LOW;
    digitalWrite(channelOrder[i], value);
  }
}

void lightRandomWires(int index) {
  if (index == ACTIVE_CHANNELS) {
    for (int i = 0; i < ACTIVE_CHANNELS; i++) {
      int value = random(0, 2) > 0 ? HIGH : LOW;
      digitalWrite(channelOrder[i], value);
    }
  }
}

int getPeakToPeak() {
  unsigned long startMillis = millis();
  unsigned int signalMax = 0;
  unsigned int signalMin = 1023;
  while (millis() - startMillis < MIKE_SAMPLE_WINDOW) {
    int sample = analogRead(MIKE_OUT);
    if (sample > signalMax) {
      signalMax = sample;
    } else if (sample < signalMin) {
      signalMin = sample;
    }
  }
  return signalMax - signalMin;
}

int getAveragePeak() {
  unsigned long startMillis = millis();
  unsigned int numSamples = 0;
  int sum = 0;
  while (millis() - startMillis < MIKE_SAMPLE_WINDOW) {
    int sample = analogRead(MIKE_OUT) - MIKE_BASELINE;
    sample = abs(sample);
    numSamples++;
    sum += sample;
  }
  int value = round(sum / numSamples);
  return value;
}

int getRootMeanSquares() {
  unsigned long startMillis = millis();
  unsigned long sumOfSquares = 0;
  unsigned int numSamples = 0;
  while (millis() - startMillis < MIKE_SAMPLE_WINDOW) {
    int sample = analogRead(MIKE_OUT) - MIKE_BASELINE;
    numSamples++;
    sumOfSquares += sq(sample);
  }
  int avg = sumOfSquares / numSamples;
  int value = sqrt(avg);
  return round(value);
}

void printMode() {
  display.clearDisplay();
  display.drawBitmap(0, 0, modeImages[mode], SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
  display.display();
}

void playWireStartSequence() {
  for (int i = 0; i <= ACTIVE_CHANNELS; i++) {
    lightWiresUpToIndex(i);
    delay(50);
  }
  for (int i = ACTIVE_CHANNELS; i >= 0; i--) {
    lightWiresUpToIndex(i);
    delay(50);
  }
  for (int i = 0; i < 20; i++) {
    lightWiresUpToIndex(0);
    delay(25);
    lightWiresUpToIndex(ACTIVE_CHANNELS);
    delay(25);
  }
}

void initDisplay() {
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay();
  display.drawBitmap(0, 0, modeImages[mode], SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
  display.display();
}

ISR(PCINT1_vect) {
  static unsigned long lastTime = 0;
  unsigned long currentTime = millis();
  if (currentTime - lastTime > 100) {
    if (digitalRead(A2) == HIGH) {
      buttonPressed = true;
    }
  }
  lastTime = currentTime;
}
